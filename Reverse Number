import java.util.*;
public class ReverseNum{
    public static void main(String args[]){

        System.out.println("Enter a number: ");
        Scanner obj = new Scanner(System.in);
        int n = obj.nextInt();

        int reverse = 0;
        while(n>0){
            int rem = n % 10;
            n = n/10;
            reverse = reverse *10 + rem;
        }
            System.out.println(reverse);
    }

}
